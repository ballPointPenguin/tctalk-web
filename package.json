  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:css": "stylelint src",
    "lint:css:fix": "stylelint --fix src",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint --fix .",
